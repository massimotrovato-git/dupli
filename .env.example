# ── Core ──────────────────────────────────────────────
APP_ENV=dev
APP_BASE_URL=https://localhost
APP_SECRET_KEY=change-me-to-a-random-string

# ── Postgres ──────────────────────────────────────────
POSTGRES_DB=dupli
POSTGRES_USER=dupli
POSTGRES_PASSWORD=dupli_password_change
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# ── Redis ─────────────────────────────────────────────
REDIS_URL=redis://redis:6379/0

# ── Keycloak ──────────────────────────────────────────
KEYCLOAK_REALM=dupli
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin_change_me
KEYCLOAK_URL=http://keycloak:8080

# ── oauth2-proxy (OIDC -> Keycloak) ──────────────────
OAUTH2_PROXY_CLIENT_ID=oauth2-proxy
OAUTH2_PROXY_CLIENT_SECRET=change-me
# Generate with: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
OAUTH2_PROXY_COOKIE_SECRET=change_me_32_bytes_base64________
OAUTH2_PROXY_EMAIL_DOMAIN=*
OAUTH2_PROXY_REDIRECT_URL=https://localhost/oauth2/callback
# Set to "false" for local dev with self-signed certs
OAUTH2_PROXY_COOKIE_SECURE=true

# ── Telegram ingest ──────────────────────────────────
TELEGRAM_API_ID=12345
TELEGRAM_API_HASH=change-me
TELEGRAM_SESSION=telegram.session
TELEGRAM_SOURCE_CHAT_ID=0   # numeric id, e.g. -100123...
TELEGRAM_SOURCE_CHAT_TITLE=signals

# ── Executors ─────────────────────────────────────────
CTRADER_ENABLED=false
MT5_GATEWAY_URL=http://mt5-gateway:8090
MT5_ENABLED=true

